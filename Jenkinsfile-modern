node ('traccar') {
   stage('Preparation') { // for display purposes
      git 'https://github.com/traccar/traccar-web.git'
      sh 'git checkout modern'
   }
   stage('Run') {
      steps {
         nodejs(nodeJSInstallationName: 'node') {
            sh "npm install"
            sh "npm start"
         }
      }
   }
}
