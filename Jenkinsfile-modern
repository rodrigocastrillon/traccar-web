node ('traccar') {
   stage('Preparation') { // for display purposes
      git branch: "modern", url: 'https://github.com/traccar/traccar-web.git'
      sh 'pwd'
      sh 'ls'
   }
   stage('Run') {
      nodejs(nodeJSInstallationName: 'node') {
         sh 'cd traccar-web'
         sh "npm install"
         sh "npm start"
      }
   }
}
